<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="net.linybin7.core.frame.bo.UserRole" table="SYS_USER_ROLE">
		<composite-id>
			<key-property name="userCode" column="USERCODE" length="200"></key-property>
			<key-property name="roleCode" column="ROLECODE" length="200"></key-property>
		</composite-id>
	</class>

</hibernate-mapping>
